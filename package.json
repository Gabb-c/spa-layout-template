{
  "name": "@mfe/utility-template",
  "types": "dist/mfe-utility-template.d.ts",
  "scripts": {
    "analyze": "webpack --mode=production --env analyze",
    "build": "webpack --mode=production",
    "postbuild": "tsc",
    "build-storybook": "build-storybook",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint src --ext js,ts,tsx",
    "lint:fix": "eslint src --ext js,ts,tsx",
    "prepare": "is-ci || husky install",
    "start": "webpack serve --port 32001",
    "start:dev": "webpack-dev-server --port 32001 --env isLocal --progress --client-progress",
    "start:standalone": "webpack serve --env standalone",
    "storybook": "start-storybook -p 6006",
    "test:ci": "jest --ci --runInBand --bail",
    "test:cov": "cross-env BABEL_ENV=test jest --coverage",
    "test:dev": "cross-env BABEL_ENV=test jest --verbose --colors --expand --maxWorkers=50% --detectOpenHandles --errorOnDeprecated",
    "test:watch": "cross-env BABEL_ENV=test jest --watch --verbose --colors --expand --maxWorkers=25%"
  },
  "dependencies": {
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/systemjs": "^6.1.1",
